# Production Environment - Real Database and API
# Replace placeholder values with your actual production credentials

# Database Connection
DATABASE_URL=postgresql://your_username:your_password@your_host:5432/rfp_platform
DB_SSL=true

# API Configuration
VITE_API_URL=https://api.your-domain.com
NODE_ENV=production

# JWT/Auth
JWT_SECRET=your-production-jwt-secret-min-32-characters-here
JWT_EXPIRY=7d

# App Config
VITE_APP_NAME=RFP Platform
VITE_APP_VERSION=2.0.0

# Optional: Third-party services
SALESFORCE_CLIENT_ID=your_salesforce_client_id
SALESFORCE_CLIENT_SECRET=your_salesforce_client_secret
HUBSPOT_API_KEY=your_hubspot_api_key
SENDGRID_API_KEY=your_sendgrid_api_key

